													<% element.ru_description.forEach(description => { %>
													<p><%=description%></p>
													<% }) %>
												</div>
												<div class="price order-2">
													<strong><%=element.price%>₪</strong>
												</div>
												<!-- .menu-food-item -->
											</div>
											<% } %> <% }) %>
										</div>
										<div class="tab-pane fade" id="beverage" role="tabpanel" aria-labelledby="pills-beverage-tab">
											<% pr.forEach(element => { %> <% if (element.tag== 'beverage') { %>
											<div class="d-block d-md-flex menu-food-item">
												<div class="text order-1 mb-3">
													<% if (element.recommended == 'true') { %>
													<i class="icon" aria-hidden="true"></i>
													<% } %>
													<h2><%=element.ru_name%></h2>
													<% element.ru_description.forEach(description => { %>
													<p><%=description%></p>
													<% }) %>
												</div>
												<div class="price order-2">
													<strong><%=element.price%>₪</strong>
												</div>
												<!-- .menu-food-item -->
											</div>
											<% } %> <% }) %>
										</div>
										<div class="tab-pane fade" id="cocktails" role="tabpanel" aria-labelledby="pills-cocktails-tab">
											<% pr.forEach(element => { %> <% if (element.tag== 'cocktails') { %>
											<div class="d-block d-md-flex menu-food-item">
